
# coding: utf-8

# In[1]:


import pandas as pd


# In[12]:


fnamea='../out_n.txt'
fnameb='../out_t.txt'


# In[2]:


dfn=pd.read_csv('../out_n.txt')
dft=pd.read_csv('../out_t.txt')


# In[7]:


coln=dfn.columns
colt=dft.columns
col=[col for col in coln if col in colt]


# In[8]:


dfn=dfn[col]
dft=dft[col]


# In[9]:


dfn.head()


# In[10]:


dft.head()


# In[13]:


dfn.to_csv(fnamea.replace('.txt','')+'matched.csv',index=None)


# In[14]:


dft.to_csv(fnameb.replace('.txt','')+'matched.csv',index=None)

